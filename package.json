{
  "name": "le-bon-meeple",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "api:build": "nest build",
    "lint": "eslint \"apps/**/*.ts\" \"apps/**/test/**/*.ts\"",
    "lint:fix": "eslint \"apps/**/*.ts\" \"apps/**/test/**/*.ts\" --fix",
    "format": "prettier --write \"apps/**/*.ts\" \"apps/**/test/**/*.ts\"",
    "format:check": "prettier --check \"apps/**/*.ts\" \"apps/**/test/**/*.ts\"",
    "api:start": "nest start",
    "api:start:dev": "nest start --watch",
    "api:start:debug": "nest start --debug --watch",
    "api:start:prod": "node dist/main",
    "api:test": "vitest",
    "api:test:cov": "vitest --coverage.enabled --coverage",
    "api:test:coverage": "vitest run --coverage",
    "prisma:migrate": "prisma migrate dev --schema=./apps/back-end/prisma/schema.prisma",
    "prisma:fixtures": "ts-node apps/back-end/prisma/seed.ts",
    "prisma:generate": "prisma generate --schema=./apps/back-end/prisma/schema.prisma",
    "tsc": "tsc --noEmit",
    "prepare": "husky",
    "front:dev": "next dev apps/front-end",
    "front:build": "next build",
    "front:start": "next start"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.10.9",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.0",
    "@nestjs/common": "^11.1.2",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.2",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.8.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/speakeasy": "^2.0.10",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "framer-motion": "^12.23.6",
    "next": "^15.4.4",
    "nodemailer": "^7.0.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.61.1",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "speakeasy": "^2.0.0",
    "zod": "^4.0.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.30.1",
    "@faker-js/faker": "^9.9.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.2",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.36",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-istanbul": "^3.2.4",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "prisma": "^6.8.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "tailwindcss": "^4.0.0",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.35.1",
    "vitest": "^3.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
